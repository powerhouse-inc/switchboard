{
    "name": "powerhouse-switchboard",
    "version": "0.0.1",
    "description": "Open-source API over database models made to be developer- and analyst-friendly.",
    "author": "makerdao",
    "type": "module",
    "license": "AGPL-3",
    "scripts": {
        "dev": "vite-node -w src/index.ts",
        "start": "NODE_ENV=production vite-node src/index.ts",
        "debug": "DEBUG=1 npm run dev",
        "postinstall": "prisma generate",
        "lint": "eslint \"./**/*.{ts,tsx}\" --max-warnings=0",
        "typecheck": "tsc --noEmit",
        "test": "vitest run && vitest run --config ./vitest.modules.config.ts",
        "watch": "vitest watch",
        "gql-generate": "graphql-codegen -c ./codegen/graphql.ts && vite-node ./codegen/client.ts"
    },
    "dependencies": {
        "@apollo/server": "^4.5.0",
        "@pothos/core": "^3.29.0",
        "@pothos/plugin-prisma": "^3.47.0",
        "@pothos/plugin-relay": "^3.39.0",
        "@pothos/plugin-simple-objects": "^3.6.7",
        "@prisma/client": "^4.11.0",
        "@genql/cli": "^3.0.4",
        "bcrypt": "^5.1.0",
        "body-parser": "^1.20.2",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "graphql": "^16.6.0",
        "graphql-middleware": "^6.1.33",
        "graphql-playground-middleware-express": "^1.7.23",
        "graphql-scalars": "^1.21.3",
        "graphql-yoga": "^3.8.0",
        "jsonwebtoken": "^9.0.0",
        "ms": "^2.1.3",
        "nexus": "^1.3.0",
        "nexus-validation-plugin": "^0.2.0",
        "pino": "^8.11.0",
        "pino-http": "^8.3.3",
        "pino-pretty": "^10.0.0",
        "vite-node": "^0.29.2",
        "vitest": "^0.29.2",
        "zod": "^3.21.4"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "^3.2.2",
        "@graphql-codegen/client-preset": "^2.1.1",
        "@types/bcrypt": "^5.0.0",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/jsonwebtoken": "^9.0.1",
        "@types/ms": "^0.7.31",
        "@types/node": "^18.14.2",
        "@typescript-eslint/eslint-plugin": "^5.54.0",
        "@typescript-eslint/parser": "^5.54.0",
        "@vitest/coverage-istanbul": "^0.29.2",
        "eslint": "^8.35.0",
        "eslint-config-airbnb-base": "^15.0.0",
